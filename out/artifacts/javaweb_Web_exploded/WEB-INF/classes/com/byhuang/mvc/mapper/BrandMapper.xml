<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.byhuang.mvc.mapper.BrandMapper">

    <resultMap id="resultMap" type="com.byhuang.pojo.Brand">
        <result column="ent_name" property="entName"></result>
    </resultMap>

    <select id="selectAllBrands" resultMap="resultMap">
        select * from brand;
    </select>
</mapper>